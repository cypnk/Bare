{	
	"realms"	: [
		{
			"domain"	: "localhost",
			"alias"		: [ "test.local", "test.dev" ],
			
			"main_links"	: [
				{ "url" : "{home}about", "text" : "{lang:nav:about}" },
				{ "url" : "{home}archive", "text" : "{lang:nav:archive}" },
				{ "url" : "{feedlink}", "text" : "{lang:nav:feed}" }
			],
			
			"about_links" : [
				{ "url" : "{home}", "text" : "{lang:nav:home}" },
				{ "url" : "{home}archive", "text" : "{lang:nav:archive}" },
				{ "url" : "{feedlink}", "text" : "{lang:nav:feed}" }
			], 
			
			"footer_links" : [
				{ "url" : "{home}about", "text" : "{lang:nav:about}" },
				{ "url" : "{home}archive", "text" : "{lang:nav:archive}" },
				{ "url" : "{feedlink}", "text" : "{lang:nav:feed}" }
			], 
			
			"meta_links"	: [
				{ 
					"name"		: "generator", 
					"content"	: "Bare; https:\/\/github.com\/cypnk\/Bare" 
				}
			],
		}
	],
	
	"defaults" : {
		"app"			: "Bare",
		"author"		: "admin",
		"storage_dir"		: "cache",
		"post_dir"		: "posts",
		"template_dir"		: "templates",
		"date_format"		: "l, F j, Y",
		"related_limit"		: 5,
		"max_page"		: 500,
		"max_posts"		: 1000,
		"posts_per_page"	: 10,
		"cache_ttl"		: 3600,
		"max_log_size"		: 5000000,
		"entry_ext"		: "md",
		"entry_meta_lines"	: 10,
		"entry_fields"		: [ "title", "tags", "summary", "author", "updated" ],
		"session_regen"		: 1800,
		"mail_from"		: "domain@localhost",
		"timezone"		: "America\/New_York",
		"language"		: "en-US",
		"static_ext"		: {
			"text"		: [ "css", "js", "txt", "html", "vtt" ],
			"images"	: [ "ico", "jpg", "jpeg", "gif", "bmp", "png", "tif", "tiff", "svg", "webp" ], 
			"fonts"		: [ "ttf", "otf", "woff", "woff2" ],
			"audio"		: [ "ogg", "oga", "mpa", "mp3", "m4a", "wav", "wma", "flac" ],
			"video"		: [ "avi", "mp4", "mkv", "mov", "ogg", "ogv" ],
			"documents"	: [ "doc", "docx", "ppt", "pptx", "pdf", "epub" ],
			"archives"	: [ "zip", "rar", "gz", "tar" ]
		},
		"frame_whitelist" : [
			"https:\/\/www.youtube.com",
			"https:\/\/player.vimeo.com",
			"https:\/\/archive.org",
			"https:\/\/lbry.tv",
			"https:\/\/odysee.com",
			"https:\/\/playeur.com"
		],
		"headers"	: {
			"Permissions-Policy" : {
				"separator"	: ", ",
				"joiner"	: "=",
				"items"		: {
					"accelerometer"		: "(none)",
					"camera"		: "(none)",
					"fullscreen"		: "(self)",
					"geolocation"		: "(none)",
					"gyroscope"		: "(none)",
					"interest-cohort"	: "()",
					"payment"		: "(none)",
					"usb"			: "(none)",
					"microphone"		: "(none)",
					"magnetometer"		: "(none)"
				}
			},
			"Content-Security-Policy": [
				"default-src 'self'",
				"script-src 'self'",
				"style-src 'self'",
				"img-src 'self' data:",
				"font-src 'self'",
				"object-src 'none'",
				"frame-ancestors 'none'"
			],
			"Referrer-Policy" : [
				"no-referrer",
				"strict-origin-when-cross-origin"
			],
			"Strict-Transport-Security" : [
				"max-age=63072000",
				"includeSubDomains"
			],
			"X-Content-Type-Options" : "nosniff",
			"X-Frame-Options" : "SAMEORIGIN",
			"X-XSS-Protection" : [
				"1",
				"mode=block"
			]
		},
		
		"tag_map"	: {
